	},
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@types/react": "17.0.30",
		"@types/react-modal": "^3.16.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"eslint": "8.2.0",
		"eslint-config-airbnb": "19.0.2",
		"eslint-config-next": "^13.0.6",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^8.0.2",
		"jest": "^27.3.1",
