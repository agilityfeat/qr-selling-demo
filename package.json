{
  "name": "front-template",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^13.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.43.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "husky": "^8.0.2",
		"lint-staged": "^13.1.0",
    "typescript": "4.4.4"
  },
  "husky": {
		"hooks": {
			"pre-commit": "yarn run lint-staged"
		}
	},
  "engines": {
    "node": ">=13 <=18"
  }
}